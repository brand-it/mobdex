<div id="subNavi">
	<%= link_to "New Domain", new_domain_path %>
</div>
<div id="search">
	<%= form_tag domains_path, :method => :get do %>
		<div><%= search_field_tag :search, params[:search] %></div>
	<% end %>
</div>
<div id="information">
	<% for domain in @domains %>
		<div>
			<%= link_to domain.url, edit_domain_path(domain) %>
			<%= link_to "Show", domain_path(domain) %>
			<%= link_to "Delete", domain_path(domain), :method => :delete %>
		</div>
	<% end %>
</div>