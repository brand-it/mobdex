<div class="content-box"><!-- Start Content Box -->
	<div class="content-box-header">
		<h3>Add Feedback for <i><%= @domain.url %></i></h3>
		<div class="clear"></div>
	</div> <!-- End .content-box-header -->
	<div class="content-box-content">
		<div class="tab-content default-tab" id="tab1"> <!-- This is the target div. id must match the href of this div's tab -->
			<% @feedbacks.each do |feedback| %>
				<p>This note was created <%= time_ago_in_words feedback.created_at %></p>
				<div style="width: 95%; height: 200px; overflow-x:hidden; overflow-y:scroll; background: #F2F2F2; padding: 15px; font-family: courier; ">
					<%= textilize feedback.note %>
				</div>
			<% end %>

		</div> <!-- End #tab1 -->    
	</div> <!-- End .content-box-content -->
</div> <!-- End .content-box -->